{
	"name" : "runphp/ad",
	"description" : "A Laravel app for advertisement.",
	"keywords" : [
		"laravel",
		"advertisement"
	],
	"license" : "MIT",
	"type" : "project",
	"require" : {
		"laravel/framework" : "5.1.*",
		"socialiteproviders/qq": "1.0.1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4.0",
		"phpspec/phpspec" : "~2.1",
		"fabpot/php-cs-fixer" : "1.8.1"
	},
	"autoload" : {
		"classmap" : [
			"database"
		],
		"psr-4" : {
			"App\\" : "app/"
		}
	},
	"autoload-dev" : {
		"classmap" : [
			"tests/TestCase.php"
		]
	},
	"scripts" : {
		"post-install-cmd" : [
			"php artisan clear-compiled",
			"php artisan optimize",
			"cp .env.example .env",
			"php artisan migrate"
		],
		"post-update-cmd" : [
			"php artisan clear-compiled",
			"php artisan optimize",
			"vendor/bin/php-cs-fixer fix --config-file=\".php_cs\""
		]
	},
	"config" : {
		"preferred-install" : "dist"
	},
	"authors" : [{
			"email" : "runphp@qq.com",
			"name" : "heui"
		}
	],
	"support" : {
		"email" : "runphp@qq.com"
	}
}